{
  "name": "eslint-ast-utils",
  "version": "1.1.0",
  "description": "Utility library to manipulate ASTs",
  "license": "MIT",
  "repository": "jfmengels/eslint-ast-utils",
  "author": {
    "name": "Jeroen Engels",
    "email": "jfm.engels@gmail.com",
    "url": "github.com/jfmengels"
  },
  "engines": {
    "node": ">=4"
  },
  "scripts": {
