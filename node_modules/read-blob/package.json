{
  "name": "read-blob",
  "version": "1.1.0",
  "description": "Read file blobs without going insane",
  "main": "index.js",
  "testling": {
    "files": "test.js",
    "browsers": [
      "ie/9..latest",
      "chrome/latest",
      "firefox/38..latest",
      "safari/latest",
      "opera/11.0..latest",
      "iphone/6..latest",
      "ipad/6..latest",
      "android-browser/latest"
    ]
  },
  "scripts": {
    "build": "browserify . -s readBlob > dist/read-blob.js",
    "prepublish": "npm test && npm run build",
    "test": "semistandard && hihat test.js --exec --timeout 1000"
  },
  "keywords": [
    "file",
    "blob",
    "arraybuffer",
    "dataurl",
    "browser"
  ],
  "author": "Will Binns-Smith <wbinnssmith@gmail.com> (http://wbinnssmith.com)",
  "license": "MIT",
  "devDependencies": {
    "arraybuffer-equal": "^1.0.2",
    "base-64": "^0.1.0",
    "blue-tape": "^0.1.10",
    "browserify": "^11.2.0",
    "hihat": "^2.4.3",
    "lodash.isequal": "^3.0.4",
    "semistandard": "^7.0.2"
  }
}
